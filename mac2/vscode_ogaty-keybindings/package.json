{
  "name": "ogaty-keymap",
  "displayName": "Ogaty Keymap",
  "description": "ogaty keymap with snippet",
  "version": "0.7.1",
  "publisher": "Yuji Ogata",
  "engines": {
    "vscode": "^1.70.0"
  },
  "repository": "https://git.jetbrains.space/ogatism/others/ogaty-keybindings.git",
  "icon": "icon.png",
  "categories": [
    "Keymaps"
  ],
  "contributes": {
    "keybindings": [
      {
        "key": "cmd+l",
        "command": "-expandLineSelection",
        "when": "textInputFocus"
      },
      {
        "key": "shift+cmd+,",
        "command": "-editor.action.inPlaceReplace.up",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "key": "shift+cmd+.",
        "command": "-breadcrumbs.focusAndSelect",
        "when": "breadcrumbsPossible && breadcrumbsVisible"
      },
      {
        "key": "shift+cmd+.",
        "command": "-editor.action.inPlaceReplace.down",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "key": "shift+cmd+.",
        "command": "-breadcrumbs.toggleToOn",
        "when": "!config.breadcrumbs.enabled"
      },
      {
        "key": "shift+cmd+[Semicolon]",
        "command": "-workbench.action.zoomIn"
      },
      {
        "key": "shift+cmd+[Minus]",
        "command": "-workbench.action.zoomIn"
      },
      {
        "key": "cmd+numpad_subtract",
        "command": "-workbench.action.zoomOut"
      },
      {
        "key": "cmd+[IntlRo]",
        "command": "-workbench.action.zoomOut"
      },
      {
        "key": "cmd+numpad_add",
        "command": "-workbench.action.zoomIn"
      },
      {
        "key": "cmd+numpad0",
        "command": "-workbench.action.zoomReset"
      },
      {
        "key": "cmd+d",
        "command": "-editor.action.addSelectionToNextFindMatch",
        "when": "editorFocus"
      },
      {
        "key": "cmd+k cmd+j",
        "command": "-editor.unfoldAll",
        "when": "editorTextFocus && foldingEnabled"
      },
      {
        "key": "cmd+k cmd+k",
        "command": "-editor.action.selectFromAnchorToCursor",
        "when": "editorTextFocus && selectionAnchorSet"
      },
      {
        "key": "cmd+[Minus]",
        "command": "workbench.action.navigateBack",
        "when": "canNavigateBack && isMac"
      },
      {
        "key": "shift+cmd+[Minus]",
        "command": "workbench.action.navigateForward",
        "when": "canNavigateForward && isMac"
      },
      {
        "key": "shift+cmd+[Semicolon]",
        "command": "workbench.action.navigateForward",
        "when": "canNavigateForward && isMac"
      },
      {
        "key": "shift+cmd+.",
        "command": "editor.action.fontZoomIn",
        "when": "isMac"
      },
      {
        "key": "shift+cmd+,",
        "command": "editor.action.fontZoomOut",
        "when": "isMac"
      },
      {
        "key": "shift+cmd+[IntlRo]",
        "command": "editor.emmet.action.wrapWithAbbreviation",
        "when": "isMac"
      },
      {
        "key": "cmd+k cmd+5",
        "command": "editor.action.jumpToBracket",
        "when": "editorTextFocus && isMac"
      },
      {
        "key": "cmd+k cmd+6",
        "command": "editor.emmet.action.matchTag",
        "when": "isMac"
      },
      {
        "key": "cmd+d",
        "command": "editor.action.duplicateSelection",
        "when": "editorFocus && isMac"
      },
      {
        "key": "cmd+j",
        "command": "editor.action.joinLines",
        "when": "editorTextFocus && !editorReadonly && isMac"
      },
      {
        "key": "cmd+k cmd+j",
        "command": "deleteAllLeft",
        "when": "editorTextFocus && !editorReadonly && isMac"
      },
      {
        "key": "cmd+k cmd+k",
        "command": "deleteAllRight",
        "when": "editorTextFocus && !editorReadonly && isMac"
      },
      {
        "key": "ctrl+k",
        "command": "editor.action.deleteLines",
        "when": "editorTextFocus && !editorReadonly && isMac"
      },
      {
        "key": "cmd+l",
        "command": "workbench.action.gotoLine",
        "when": "isMac"
      },
      {
        "key": "ctrl+g",
        "command": "-workbench.action.gotoLine",
        "when": "isMac"
      },
      {
          "key": "cmd+r",
          "command": "editor.action.startFindReplaceAction",
          "when": "editorFocus || editorIsOpen && isMac"
      },
      {
        "key": "cmd+e",
        "command": "-editor.action.findWithSelection",
        "when": "isMac"
      },
      {
        "key": "cmd+e",
        "command": "workbench.action.openPreviousEditorFromHistory",
        "when": "isMac"
      },
      {
        "key": "alt+cmd+t",
        "command": "-workbench.action.closeOtherEditors",
        "when": "isMac"
      },
      {
        "key": "ctrl+oem_minus",
        "command": "workbench.action.navigateBack",
        "when": "canNavigateBack && isWindows"
      },
      {
        "key": "ctrl+oem_minus",
        "command": "-workbench.action.zoomOut"
      },
      {
        "key": "ctrl+shift+oem_minus",
        "command": "workbench.action.navigateForward",
        "when": "canNavigateForward && isWindows"
      },
      {
        "key": "ctrl+shift+oem_minus",
        "command": "-workbench.action.zoomOut"
      },
      {
        "key": "ctrl+l",
        "command": "workbench.action.gotoLine",
        "when": "isWindows"
      },
      {
          "key": "ctrl+g",
          "command": "-workbench.action.gotoLine",
          "when": "isWindows"
      },
      {
          "key": "ctrl+l",
          "command": "-expandLineSelection",
          "when": "textInputFocus && isWindows"
      },
      {
          "key": "ctrl+l",
          "command": "-workbench.action.chat.newChat",
          "when": "hasChatProvider && inChat && isWindows"
      },
      {
        "key": "ctrl+shift+9",
        "command": "editor.action.fontZoomIn",
        "when": "true"
      },
      {
          "key": "shift+cmd+.",
          "command": "-editor.action.fontZoomIn",
          "when": "true"
      },
      {
          "key": "ctrl+shift+7",
          "command": "editor.action.fontZoomOut",
          "when": "true"
      },
      {
        "key": "shift+cmd+,",
        "command": "-editor.action.fontZoomOut",
        "when": "true"
      }
    ],
    "snippets": [
        {
            "language": "html",
            "path": "./snippets/html.json"
        }
    ]
  }
}
