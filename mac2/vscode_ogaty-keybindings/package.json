{
  "name": "ogaty-keymap",
  "displayName": "Ogaty Keymap",
  "description": "ogaty keymap with snippet",
  "version": "1.2.0",
  "publisher": "Yuji Ogata",
  "engines": {
    "vscode": "^1.70.0"
  },
  "repository": "https://git.jetbrains.space/ogatism/others/ogaty-keybindings.git",
  "icon": "icon.png",
  "categories": [
    "Keymaps"
  ],
  "contributes": {
    "keybindings": [
      {
        "key": "cmd+5",
        "command": "-workbench.action.focusFifthEditorGroup"
      },
      {
        "key": "cmd+6",
        "command": "-workbench.action.focusSixthEditorGroup"
      },
      {
        "key": "cmd+l",
        "command": "-expandLineSelection",
        "when": "textInputFocus"
      },
      {
        "key": "shift+cmd+,",
        "command": "-editor.action.inPlaceReplace.up",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "key": "shift+cmd+.",
        "command": "-breadcrumbs.focusAndSelect",
        "when": "breadcrumbsPossible && breadcrumbsVisible"
      },
      {
        "key": "shift+cmd+.",
        "command": "-editor.action.inPlaceReplace.down",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "key": "shift+cmd+.",
        "command": "-breadcrumbs.toggleToOn",
        "when": "!config.breadcrumbs.enabled"
      },
      {
        "key": "shift+cmd+[Semicolon]",
        "command": "-workbench.action.zoomIn"
      },
      {
        "key": "shift+cmd+[Minus]",
        "command": "-workbench.action.zoomIn"
      },
      {
        "key": "cmd+numpad_subtract",
        "command": "-workbench.action.zoomOut"
      },
      {
        "key": "cmd+[IntlRo]",
        "command": "-workbench.action.zoomOut"
      },
      {
        "key": "cmd+numpad_add",
        "command": "-workbench.action.zoomIn"
      },
      {
        "key": "cmd+numpad0",
        "command": "-workbench.action.zoomReset"
      },
      {
        "key": "cmd+d",
        "command": "-editor.action.addSelectionToNextFindMatch",
        "when": "editorFocus"
      },
      {
        "key": "cmd+k cmd+l",
        "command": "-editor.toggleFold",
        "when": "editorTextFocus && foldingEnabled"
      },
      {
          "key": "cmd+k cmd+k",
          "command": "editor.action.deleteLines",
          "when": "textInputFocus && !editorReadonly"
      },
      {
          "key": "cmd+k cmd+l",
          "command": "deleteAllRight",
          "when": "editorTextFocus && !editorReadonly"
      },
      {
          "key": "cmd+k cmd+k",
          "command": "-deleteAllRight",
          "when": "editorTextFocus && !editorReadonly"
      },
      {
        "win": "ctrl+[Minus]",
        "mac": "cmd+[Minus]",
        "key": "cmd+[Minus]",
        "command": "workbench.action.navigateBack",
        "when": "canNavigateBack"
      },
      {
        "win": "shift+ctrl+[Minus]",
        "mac": "shift+cmd+[Minus]",
        "key": "shift+cmd+[Minus]",
        "command": "workbench.action.navigateForward",
        "when": "canNavigateForward"
      },
      {
        "win": "shift+ctrl+[Semicolon]",
        "mac": "shift+cmd+[Semicolon]",
        "key": "shift+cmd+[Semicolon]",
        "command": "workbench.action.navigateForward",
        "when": "canNavigateForward"
      },
      {
        "win": "shift+ctrl+.",
        "mac": "shift+cmd+.",
        "key": "shift+cmd+.",
        "command": "editor.action.fontZoomIn"
      },
      {
        "win": "shift+ctrl+,",
        "mac": "shift+cmd+,",
        "key": "shift+cmd+,",
        "command": "editor.action.fontZoomOut"
      },
      {
        "win": "shift+ctrl+[IntlRo]",
        "mac": "shift+cmd+[IntlRo]",
        "key": "shift+cmd+[IntlRo]",
        "command": "editor.emmet.action.wrapWithAbbreviation"
      },
      {
        "win": "ctrl+5",
        "mac": "cmd+5",
        "key": "cmd+5",
        "command": "editor.action.jumpToBracket",
        "when": "editorTextFocus"
      },
      {
        "win": "ctrl+6",
        "mac": "cmd+6",
        "key": "cmd+6",
        "command": "editor.emmet.action.matchTag"
      },
      {
        "win": "ctrl+d",
        "mac": "cmd+d",
        "key": "cmd+d",
        "command": "editor.action.duplicateSelection",
        "when": "editorFocus"
      },
      {
        "win": "ctrl+j",
        "mac": "cmd+j",
        "key": "cmd+j",
        "command": "editor.action.joinLines",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "win": "ctrl+k ctrl+j",
        "mac": "cmd+k cmd+j",
        "key": "cmd+k cmd+j",
        "command": "deleteAllLeft",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "win": "ctrl+k ctrl+k",
        "mac": "cmd+k cmd+k",
        "key": "cmd+k cmd+k",
        "command": "deleteAllRight",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "win": "ctrl+l",
        "mac": "cmd+l",
        "key": "cmd+l",
        "command": "workbench.action.gotoLine"
      },
      {
        "win": "ctrl+g",
        "mac": "cmd+g",
        "key": "ctrl+g",
        "command": "-workbench.action.gotoLine"
      },
      {
        "win": "ctrl+r",
        "mac": "cmd+r",
        "key": "cmd+r",
        "command": "editor.action.startFindReplaceAction",
        "when": "editorFocus || editorIsOpen"
      },
      {
        "win": "ctrl+e",
        "mac": "cmd+e",
        "key": "cmd+e",
        "command": "-editor.action.findWithSelection"
      },
      {
        "win": "ctrl+e",
        "mac": "cmd+e",
        "key": "cmd+e",
        "command": "workbench.action.openPreviousEditorFromHistory"
      },
      {
        "win": "alt+ctrl+t",
        "mac": "alt+cmd+t",
        "key": "alt+cmd+t",
        "command": "-workbench.action.closeOtherEditors"
      },
      {
        "win": "ctrl+oem_minus",
        "key": "ctrl+oem_minus",
        "command": "workbench.action.navigateBack",
        "when": "canNavigateBack"
      },
      {
        "win": "ctrl+oem_minus",
        "key": "ctrl+oem_minus",
        "command": "-workbench.action.zoomOut"
      },
      {
        "win": "ctrl+shift+oem_minus",
        "key": "ctrl+shift+oem_minus",
        "command": "workbench.action.navigateForward",
        "when": "canNavigateForward"
      },
      {
        "win": "ctrl+shift+oem_minus",
        "key": "ctrl+shift+oem_minus",
        "command": "-workbench.action.zoomOut"
      },
      {
        "key": "ctrl+shift+9",
        "command": "editor.action.fontZoomIn",
        "when": "true"
      },
      {
          "key": "shift+cmd+.",
          "command": "-editor.action.fontZoomIn",
          "when": "true"
      },
      {
          "key": "ctrl+shift+7",
          "command": "editor.action.fontZoomOut",
          "when": "true"
      },
      {
        "key": "shift+cmd+,",
        "command": "-editor.action.fontZoomOut",
        "when": "true"
      }
    ],
    "snippets": [
        {
            "language": "html",
            "path": "./snippets/html.json"
        }
    ]
  }
}
